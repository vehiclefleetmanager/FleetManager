<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="leftMenu" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      spacing="3.0" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="pl.domain.company.controllers.main.LeftMenuController">

    <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            mnemonicParsing="false" onAction="#showVehicles" prefHeight="50.0" prefWidth="105.0" text="%vehicles"
            textAlignment="CENTER" wrapText="true"/>
    <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            mnemonicParsing="false" onAction="#editVehicle" prefHeight="50.0" prefWidth="105.0" text="%documents"
            textAlignment="CENTER" wrapText="true"/>
    <Separator maxHeight="5.0" minHeight="5.0"/>
    <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            mnemonicParsing="false" onAction="#openUsers" prefHeight="50.0" prefWidth="105.0" text="%users.program"
            textAlignment="CENTER" wrapText="true"/>
    <Separator maxHeight="5.0" minHeight="5.0"/>
    <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            mnemonicParsing="false" onAction="#openAbout" prefHeight="50.0" prefWidth="105.0" text="%about.program"
            textAlignment="CENTER" wrapText="true"/>

    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>